{
    "version": 2,
    "name": "repodata",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/api-docs/(.*)",
            "dest": "index.js"
          }
    ]
}