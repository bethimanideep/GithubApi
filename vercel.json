{
    "version": 2,
    "name": "github-api",
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api-docs",
        "dest": "swagger.yaml"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  